{
    "name": "ejrsilver/enforce_allman_style",
    "description": "Allman style CodeSniffer sniff for all control statements and functions.",
    "homepage": "https://github.com/ejrsilver/enforce_allman_style",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Ethan Silver",
            "email": "ejrsilver@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "phpcs",
        "standards",
        "static analysis",
        "allman"
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": ">=5.4.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "ejrsilver\\EnforceAllmanStyle\\": "src/"
        }
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "suggest": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "prefer-stable": true
}
